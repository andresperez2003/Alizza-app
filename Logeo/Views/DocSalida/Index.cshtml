
@model List<Documento.Models.ViewModels.ListDocSalida>
@{
    ViewBag.Title = "Registro de Salida de Documentos";
}

<br />
<br />
<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px">
    <a href="~/DocSalida/Nuevo" class="btn btn-primary">Nuevo</a>
</div>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Radicado</th>
                <th>Fecha de Salida</th>
                <th>ID de Destinatario</th>
                <th>ID de Dependencia</th>
                <th></th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Radicado_Salida</td>
                    <td>@oElemento.FechaSalida</td>
                    <td>@oElemento.IDDestinatario</td>
                    <td>@oElemento.IDDependencia</td>
                    <td>
                        <a class="btn btn-default" href="~/DocSalida/Editar?radicado=@oElemento.Radicado_Salida">Editar</a>
                        <a class="btn btn-danger" href="~/DocSalida/Eliminar?radicado=@oElemento.Radicado_Salida">Eliminar</a>
                    </td>
                </tr>
            }


        </table>

    </div>
</div>

